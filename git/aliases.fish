alias gb="git branch"
alias gba="git branch -a"
alias gbv="git branch -vvv"
alias gc="git commit -v"
alias gca="git commit -v -a -m"
alias gcane="git commit --amend --no-edit"
alias gci="git commit -v -m"
alias gco="git checkout"
alias gcount="git shortlog -sn"
alias gdc="git diff --cached"
alias gdf="git diff HEAD -- . ':(exclude)package-lock.json'"
alias gdt="git difftool HEAD"
alias gfr="git fetch --all && git rebase upstream/master"
alias gg="git grep"
alias gl="git pull --rebase"
alias gp="git push"
alias gpf="git push --force"
alias gpo="git push origin (current_branch)"
alias gpof="git push --force origin (current_branch)"
alias gpu="git push --set-upstream origin (current_branch)"
alias gpr="git pull-request"
alias gprd="git pull-request -d"
alias grh="git reset HEAD"
alias gsh="git ksshow"
alias gst="git status -sb"
alias gsu="git submodule update"
alias gu="git restore --staged"
