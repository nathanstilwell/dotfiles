[ui]
ssh = ssh -C
ignore = .hgignore

[alias]
latest = log --stat --limit 5
st = stat
log = log --stat

[extensions]
prompt = ~/.oh-my-zsh/plugins/mercurial/hg-prompt/prompt.py

[web]
cacerts = /etc/hg-dummy-cert.pem

[paths]
default-push = ssh://hg@bitbucket.org/nathanstilwell/sublime-packages

%include ~/.hgrc-extras
